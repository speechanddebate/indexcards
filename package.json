{
	"name": "indexcards",
	"version": "1.2.0",
	"description": "Tabroom.com Node API.  This version is the branch for the rewrite.",
	"main": "app.js",
	"type": "module",
	"start": "NODE_ENV='production' NODE_OPTIONS='--max_old_space_size=200 --experimental-vm-modules --experimental-specifier-resolution=node' node --use-strict app.js",
	"scripts": {
		"start": "NODE_ENV='production' NODE_OPTIONS='--max_old_space_size=200 --experimental-vm-modules --experimental-specifier-resolution=node' node --use-strict app.js",
		"production": "NODE_ENV='production' NODE_OPTIONS='--max_old_space_size=200' pm2 -i 16 start app.js --name indexcards --node-args='--experimental-specifier-resolution=node --use_strict' -e /var/log/indexcards/error.log",
		"staging": "NODE_ENV='staging' NODE_OPTIONS='--max_old_space_size=1000' pm2 -i 4 start app.js --name indexcards --node-args='--experimental-specifier-resolution=node --use_strict' -o /var/log/indexcards/output.log -e /var/log/indexcards/error.log ",
		"build": "npm run build:openapi",
		"build:openapi": "node scripts/build-openapi.js",
		"dev": "npm run build && TZ='UTC' NODE_ENV='development' NODE_OPTIONS='--experimental-vm-modules --experimental-specifier-resolution=node' nodemon --use-strict app.js",
		"test": "NODE_ENV='test' NODE_OPTIONS='--experimental-vm-modules --no-warnings --experimental-specifier-resolution=node' VITEST_SEGFAULT_RETRY=3 vitest watch",
		"test-ci": "NODE_ENV='test' NODE_OPTIONS='--experimental-vm-modules --no-warnings --experimental-specifier-resolution=node' VITEST_SEGFAULT_RETRY=3 vitest --watch false --run",
		"cover": "NODE_ENV='test' NODE_OPTIONS='--experimental-vm-modules --no-warnings --experimental-specifier-resolution=node' VITEST_SEGFAULT_RETRY=3 vitest --coverage",
		"dev-debug": "node --inspect-brk=9229 --experimental-vm-modules --experimental-specifier-resolution=node --enable-source-maps app.js",
		"lint": "eslint .",
		"lint-fix": "eslint --fix .",
		"format": "prettier --write .",
		"format-check": "prettier --check .",
		"prepare": "husky",
		"models": "NODE_ENV='development' node api/data/auto.js"
	},
	"extensionless": {
		"lookFor": [
			"js",
			"json"
		],
		"resolveDirs": true
	},
	"repository": {
		"type": "git",
		"url": "git+https : //github.com/speechanddebate/tabroom.git"
	},
	"keywords": [
		"speech",
		"debate",
		"congress",
		"tabulation",
		"forensics",
		"utabroom",
		"speechanddebate",
		"NSDA"
	],
	"author": "Chris Palmer",
	"license": "RPL-1.5",
	"bugs": {
		"url": "https://github.com/speechanddebate/tabroom/issues"
	},
	"engines": {
		"node": ">= 24.13.0"
	},
	"homepage": "https://www.tabroom.com/about",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.397.0",
		"@google/genai": "^1.28.0",
		"@maxmind/geoip2-node": "^3.2.0",
		"@scalar/express-api-reference": "^0.8.30",
		"@speechanddebate/nsda-js-utils": "^1.0.15",
		"authorizenet": "^1.0.10",
		"axios": "^1.5.1",
		"basic-auth": "^2.0.1",
		"body-parser": "^1.18.3",
		"bottleneck": "^2.19.0",
		"cookie-parser": "^1.4.4",
		"cors": "^2.8.5",
		"crypto-js": "^4.0.0",
		"csv-stringify": "^5.3.0",
		"dayjs": "^1.11.13",
		"dotenv": "^8.2.0",
		"express": "^5.2.1",
		"express-rate-limit": "^3.4.1",
		"express-winston": "^4.2.0",
		"full-name-splitter": "^1.1.1",
		"helmet": "^8",
		"html-to-text": "^9.0.5",
		"human-readable-ids": "^1.0.4",
		"isomorphic-fetch": "^3.0.0",
		"jsforce": "^3.10.2",
		"jsonwebtoken": "^9.0.0",
		"luxon": "^3.7.2",
		"mariadb": "^2.3.3",
		"node-fetch": "^3.3.1",
		"nodemailer": "^7.0.9",
		"papaparse": "^5.1.0",
		"sequelize": "^6.37.7",
		"sql-template-strings": "^2.2.2",
		"ua-parser-js": "^0.7.28",
		"unixcrypt": "^3.0.4",
		"uuid": "^7",
		"winston": "^3.2.1",
		"xml2js": "^0.6.2"
	},
	"devDependencies": {
		"@alt3/sequelize-to-json-schemas": "^0.3.55",
		"@eslint/compat": "^1.2.5",
		"@faker-js/faker": "^10.2.0",
		"@stylistic/eslint-plugin-js": "^2.13.0",
		"@types/luxon": "^3.7.1",
		"@vitest/coverage-v8": "^4.0.17",
		"@vitest/eslint-plugin": "^1.1.25",
		"@vitest/ui": "^4.0.17",
		"chai": "^4.2.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-import-x": "^4.16.1",
		"eslint-plugin-perfectionist": "^4.7.0",
		"husky": "^9.0.11",
		"nock": "^10.0.6",
		"node-mocks-http": "^1.10.1",
		"nodemon": "^3.1.10",
		"sequelize-auto": "^0.8.8",
		"supertest": "^7.1.3",
		"ts-node": "^10.9.1",
		"tsx": "^4.19.2",
		"typescript": "^5.2.0",
		"typescript-eslint": "^8.20.0",
		"vitest": "^4.0.17"
	}
}
